    "debugger",
    "downloads",
    "history",
    "management",
    "notifications",
    "offscreen",
    "pageCapture",
    "readingList",
    "scripting",
    "search",
    "sessions",
    "storage",
    "tabGroups",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "options_ui": {
    "page": "src/options_ui/options_ui.html"
  },
