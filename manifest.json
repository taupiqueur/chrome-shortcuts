      "matches": ["<all_urls>"],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start",
      "js": [
        "src/lib/keymap.js",
        "src/lib/input_handler.js",
        "src/lib/scroller.js",
        "src/content_script.js"
      ]
    }
